{
  "name": "higgsnode",
  "version": "1.0.0",
  "description": "HiggsNode - Node application for Higgs.net decentralized VPN network",
  "main": "dist/index.js",
  "bin": {
    "higgsnode": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli/index.js start",
    "dev": "ts-node-dev --respawn --transpile-only src/cli/index.ts start",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
